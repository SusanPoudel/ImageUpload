{
    "name": "yourusername/image-upload",
    "description": "A Laravel package for handling image uploads.",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "ImageUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ImageUpload\\ImageUploadServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
